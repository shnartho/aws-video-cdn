# AWS credentials - required for Pulumi deployment and AWS SDK
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=eu-west-1

# Application settings
HOST=0.0.0.0
PORT=8000
ENV=development
LOG_LEVEL=info

# AWS resources - updated by Pulumi deployment
S3_BUCKET_NAME=video-storage-bucket-dev
CLOUDFRONT_DOMAIN=

# API settings
PROJECT_NAME=AWS Video CDN
PROJECT_DESCRIPTION=Video hosting service with AWS S3 and CloudFront CDN
PROJECT_VERSION=0.1.0
API_V1_STR=/api/v1
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:8000"]
